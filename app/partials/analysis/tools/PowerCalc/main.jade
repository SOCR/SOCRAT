    div
      input#SimpleChi2GUI_H(style='margin-bottom:10px;' type='button' value='Show Help' ng-click='mainArea.SimpleChi2GUI_show_help();')
      div(ng-show='mainArea.SimpleChi2GUI_help == true;')
